  volume = {82},
  number = {4},
  note = {Inoglu, Nilay Kitchin, John R.},
  abstract = {The adsorption energies of simple atomic adsorbates are coverage dependent.
	We use density-functional theory to show that the coverage dependence
	is due to an adsorbate-induced modification of the surface d-band
	structure. We developed a simple model for predicting the d-band
